from pip._vendor.distlib.compat import raw_input

print("hello word_测试")
if True:
    print("True")
else:
    print("False")
    # 没有严格缩进，在执行时会报错
    print("a")

#raw_input("\n\nPress the enter key to exit.")

x='abc'
y='def'
#换行输出
print (x)
print (y)
print("不换行输出")
print (x),
print (y),
print("不换行输出")
print (x,y)

str = 'Hello World!'
print (str ) # 输出字符串中的第一个字符
print (str[2:5] ) # 输出字符串中第三个至第五个之间的字符串
print (str[2:] ) # 输出从第三个字符开始的字符串
print (str * 5 ) # 输出字符串两次
print (str + "TEST" ) # 输出连接的字符串

print("+++++++++++++++++++++==")
list = ['runoob', 786, 2.23, 'john', 70.2,"99w我"]
tinylist = [123, 'john']
print (list)  # 输出完整列表
print(list[0])  # 输出列表的第一个元素
print(list[1:3])  # 输出第二个至第三个元素
print(list[2:])  # 输出从第三个开始至列表末尾的所有元素
print(tinylist * 2)  # 输出列表两次
print(list + tinylist ) # 打印组合的列表

print("++++++++++++ Python 字典++++++++++++++")
tinydict = {'name': 'john','age':33, 'dept': 'sales'}
dict = {}
dict['one'] = "This is one"
dict[2] = "This is two"
print(dict['one'])        # 输出键为'one' 的值
print(dict[2])          # 输出键为 2 的值
print(tinydict)         # 输出完整的字典
print(tinydict.keys())    # 输出所有键
print(tinydict.values())  # 输出所有值

#运算符
print("a:21 b:10 ")
a = 21
b = 10
c = 0
c = a + b
print("a + b 的值为：", c)
c = a - b
print("a - b 的值为：", c)
c = a * b
print("a * b 的值为：", c)
c = a / b
print("a / b 的值为：", c)
c = a % b
print("a % b 的值为：", c)
c = a ** b
print("a ** b 的值为：", c)
c = a // b
print("a // b的值为：", c)

#注意‘==’和is的区别
a=2.0
b=2.0# a b 都为float
if(a is b):
    print('a is b:true')
else:
    print('a is b:false')
if(a==b):
    print('a == b:true')
print(5/0.3)
a=2
b=2# a b 都为int
if(a is b):
    print('a is b:true')
else:
    print('a is b:false')
if(a==b):
    print('a == b:true')
a=123456
b=123456# a b 都为int
if(a is b):
    print('a is b:true')
else:
    print('a is b:false')
if(a==b):
    print('a == b:true')